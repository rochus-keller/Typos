let config : Config {
    .defines += [ "HAVE_CONFIG_H" "LUATEX_HARFBUZZ_ENABLED" ] # not used: "NO_DEBUG"
    if target_toolchain == `gcc {
        .lib_names += [ "m" "stdc++" ]
        .ldflags += "-shared-libgcc"
    }else if target_toolchain == `msvc {
        # nothing to do
    }else {
        error("target toolchain not supported")
    }
}

