let luatex ! : Executable {
    .configs += config;
    .deps += [ zlib zziplib lua53 libpng kpathsea web2c dvi font image lang luabind fontforge
                luafontloader luamd5 luapplib pdf slnunicode tex unilib utils mplibdir luapeg
                libmd5 synctexdir luatexdir luafilesystem luasocket luazip luaffi
                graphite2 harfbuzz luaharfbuzz ]
    # NOTE: main is in luatex.c
}
